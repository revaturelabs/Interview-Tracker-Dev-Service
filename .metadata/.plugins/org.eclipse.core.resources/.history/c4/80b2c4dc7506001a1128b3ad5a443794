package com.example.controller;

public class LoginController {
	
	 List<User> list;

	    @GetMapping(value="/users")
	    public List<User> getAll() {
	        list = new ArrayList<User>();
	        list.add(new User(1, "bodhi", "bacon"));
	        list.add(new User(1, "seacriest", "scrum"));
	        
	        return list;
	    }

}
